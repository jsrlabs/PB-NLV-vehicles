<?xml version="1.0"?>
<plotset>
  <!--
-->
  <plot>
    <title>Apogee Estimate</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Apogee estimate (miles)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">guidance/executive/apoapsis</parameter>
  </plot>

  <plot>
    <title>Perigee Estimate - zoomed</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Perigee estimate (miles)</label>
    <scale>auto</scale>
    <min axis="y">0</min>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">guidance/executive/periapsis</parameter>
  </plot>

  <plot>
    <title>Perigee Estimate</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Perigee estimate (miles)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">guidance/executive/periapsis</parameter>
  </plot>

  <plot>
    <title>Altitude</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Altitude (ft)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">Altitude AGL (ft)</parameter>
  </plot>

  <plot>
    <title>Trajectory</title>
    <label axis="x">Downrange (miles)</label>
    <label axis="y">Altitude (miles)</label>
    <scale>auto</scale>
    <!-- max axis="y">500</max -->
    <parameter axis="x">guidance/executive/downrange-miles</parameter>
    <parameter axis="y">guidance/executive/altitude-miles</parameter>
  </plot>

  <plot>
    <title>Attitude - Pitch, Roll, Yaw</title>
    <label axis="x">Time (sec)</label>
    <label axis="y"> (deg)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">Theta (deg)</parameter>
    <parameter axis="y">Phi (deg)</parameter>
    <parameter axis="y">Psi (deg)</parameter>
  </plot>
  
  <plot>
    <title>Pitch guidance table</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Pitch (rad)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">system/guidance/pitch/cmd-from-rate</parameter>
  </plot>
  <plot>
    <title>Pitch rate table</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Pitch (rad)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">system/guidance/pitch_rate_cmd_table</parameter>
  </plot>
  <plot>
    <title>Pitch error</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Pitch error (deg)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">system/guidance/pitch-error-deg</parameter>
  </plot>

  <plot>
    <title>Pitch rate cmd</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Pitch (rad/sec)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">system/guidance/pitch-rate</parameter>
  </plot>
  <plot>
    <title>Alpha</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Alpha (deg)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">Alpha (deg)</parameter>
  </plot>

  <plot>
    <title>Total Thrust</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Thrust (lbf)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">F_{Prop x} (lbs)</parameter>
  </plot>

  <plot>
    <title>Total Inertial Velocity</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Inertial Velocity (ft/sec)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">V_{Inertial} (ft/s)</parameter>
  </plot>

  <plot>
    <title>Accel</title>
    <label axis="x">Time (sec)</label>
    <label axis="y">Acceleration (ft/s2)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">accelerations/n-pilot-x-norm</parameter>
    <parameter axis="y">accelerations/n-pilot-y-norm</parameter>
    <parameter axis="y">accelerations/n-pilot-z-norm</parameter>
  </plot>

  <plot>
    <title>Q</title>
    <label axis="x">Time (sec)</label>
    <label axis="y"> (psf)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">aero/qbar-psf</parameter>
  </plot>

  <plot>
    <title>Mass Flow</title>
    <label axis="x">Time (sec)</label>
    <label axis="y"> (lb/s)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">propulsion/engine[1]/fuel-flow-rate-pps</parameter>
    <parameter axis="y">propulsion/engine[1]/oxi-flow-rate-pps</parameter>
    <parameter axis="y">propulsion/engine[8]/fuel-flow-rate-pps</parameter>
    <parameter axis="y">propulsion/engine[8]/oxi-flow-rate-pps</parameter>
    <parameter axis="y">propulsion/engine[15]/fuel-flow-rate-pps</parameter>
    <parameter axis="y">propulsion/engine[15]/oxi-flow-rate-pps</parameter>
  </plot>

  <plot>
    <title>Tank fill</title>
    <label axis="x">Time (sec)</label>
    <label axis="y"> (percent)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">propulsion/tank[1]/pct-full</parameter>
    <parameter axis="y">propulsion/tank[15]/pct-full</parameter>
    <parameter axis="y">propulsion/tank[29]/pct-full</parameter>
  </plot>

  <plot>
    <title>Thrust, Single Engine</title>
    <label axis="x">Time (sec)</label>
    <label axis="y"> (lbf)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">propulsion/engine[1]/thrust-lbs</parameter>
    <parameter axis="y">propulsion/engine[8]/thrust-lbs</parameter>
    <parameter axis="y">propulsion/engine[15]/thrust-lbs</parameter>
  </plot>

  <plot>
    <title>Tank pressure</title>
    <label axis="x">Time (sec)</label>
    <label axis="y"> (psi)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">system/fms/stage1_Ptank</parameter>
    <parameter axis="y">system/fms/stage2_Ptank</parameter>
    <parameter axis="y">system/fms/stage3_Ptank</parameter>
  </plot>

  <plot>
    <title>Tank contents</title>
    <label axis="x">Time (sec)</label>
    <label axis="y"> (lbs)</label>
    <scale>auto</scale>
    <parameter axis="x">Time</parameter>
    <parameter axis="y">propulsion/tank[1]/contents-lbs</parameter>
    <parameter axis="y">propulsion/tank[2]/contents-lbs</parameter>
    <parameter axis="y">propulsion/tank[15]/contents-lbs</parameter>
    <parameter axis="y">propulsion/tank[29]/contents-lbs</parameter>
  </plot>

</plotset>
